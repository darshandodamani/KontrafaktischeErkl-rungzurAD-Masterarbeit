# General file types to be handled by Git LFS
*.psd filter=lfs diff=lfs merge=lfs -text
*.png filter=lfs diff=lfs merge=lfs -text
*.csv filter=lfs diff=lfs merge=lfs -text
# Track PNG and CSV files in specific directories with Git LFS
dataset/town7_dataset/**/*.png filter=lfs diff=lfs merge=lfs -text
dataset/town7_dataset/**/*.csv filter=lfs diff=lfs merge=lfs -text
dataset/reconstructed_image/*.png filter=lfs diff=lfs merge=lfs -text
dataset/reconstructed_image_500_epochs_128_LF/*.png filter=lfs diff=lfs merge=lfs -text
# Track model weights with Git LFS
*.pth filter=lfs diff=lfs merge=lfs -text
town10_dataset/* filter=lfs diff=lfs merge=lfs -text
